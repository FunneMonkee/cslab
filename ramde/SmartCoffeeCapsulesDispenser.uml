  <Requirements:Requirement xmi:id="_j6xjUNVDEfCYZcqbPlE1Ng" base_NamedElement="_j6nLQNVDEfCYZcqbPlE1Ng" id="FR1" text="When a user presents an NFC card to the Reader, the system shall authenticate the user and log the user ID." base_Class="_j6nLQNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_kyi40NVDEfCYZcqbPlE1Ng" base_NamedElement="_kyeAUNVDEfCYZcqbPlE1Ng" id="FR2" text="After successful login, the system shall allow the user to select a capsule flavor." base_Class="_kyeAUNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_lpV98NVDEfCYZcqbPlE1Ng" base_NamedElement="_lpRsgNVDEfCYZcqbPlE1Ng" id="FR3" text="When a flavor is selected, the system shall send the selection and the user ID to the WWW server." base_Class="_lpRsgNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_maBM8NVDEfCYZcqbPlE1Ng" base_NamedElement="_mZ9ikNVDEfCYZcqbPlE1Ng" id="FR4" text="When capsules are present inside the dispenser, the system shall detect individual capsules using sensors." base_Class="_mZ9ikNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_nATJ0NVDEfCYZcqbPlE1Ng" base_NamedElement="_nAQGgNVDEfCYZcqbPlE1Ng" id="FR5" text="The system shall maintain an updated count of remaining capsules." base_Class="_nAQGgNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_nsh9sNVDEfCYZcqbPlE1Ng" base_NamedElement="_nsdsQNVDEfCYZcqbPlE1Ng" id="FR6" text="When capsule count becomes low, the system shall activate a low-stock warning." base_Class="_nsdsQNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_ooR0QNVDEfCYZcqbPlE1Ng" base_NamedElement="_ooOw8NVDEfCYZcqbPlE1Ng" id="FR7" text="When an operator refills the dispenser, the system shall automatically generate a refill event and notify the server." base_Class="_ooOw8NVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_pfbesNVDEfCYZcqbPlE1Ng" base_NamedElement="_pfX0UNVDEfCYZcqbPlE1Ng" id="FR8" text="The system shall periodically send capsule availability and operational status to a central MQTT server." base_Class="_pfX0UNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_rh_zYNVDEfCYZcqbPlE1Ng" base_NamedElement="_rh8wENVDEfCYZcqbPlE1Ng" id="FR9" text="When a user request is validated, the system shall control the servo motor to dispense exactly one capsule." base_Class="_rh8wENVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_sHfGENVDEfCYZcqbPlE1Ng" base_NamedElement="_sHcCwNVDEfCYZcqbPlE1Ng" id="FR10" text="The system shall perform redundant checks (e.g., multiple sensors) to detect possible failures." base_Class="_sHcCwNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_4GYhANVDEfCYZcqbPlE1Ng" base_NamedElement="_4GU2oNVDEfCYZcqbPlE1Ng" id="QR1" text="The system shall detect discrepancies between redundant sensors and trigger a failure state when inconsistencies exist." base_Class="_4GU2oNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_4v5YENVDEfCYZcqbPlE1Ng" base_NamedElement="_4v2UwNVDEfCYZcqbPlE1Ng" id="QR2" text="If a capsule is requested but the sensor count does not change, the system shall disable the dispenser and notify maintenance." base_Class="_4v2UwNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_5gUIYNVDEfCYZcqbPlE1Ng" base_NamedElement="_5gRFENVDEfCYZcqbPlE1Ng" id="QR3" text="The system shall guarantee correct transmission of user ID, selection, and capsule data to servers." base_Class="_5gRFENVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_6UTKENVDEfCYZcqbPlE1Ng" base_NamedElement="_6UQGwNVDEfCYZcqbPlE1Ng" id="QR4" text="The system shall prevent unauthorized NFC login attempts." base_Class="_6UQGwNVDEfCYZcqbPlE1Ng"/>
  <Requirements:Requirement xmi:id="_6-93sNVDEfCYZcqbPlE1Ng" base_NamedElement="_6-60YNVDEfCYZcqbPlE1Ng" id="QR5" text="The system shall ensure servo motor will not run continuously or overheat." base_Class="_6-60YNVDEfCYZcqbPlE1Ng"/>
